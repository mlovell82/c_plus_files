// object_construction.hpp
#include <iostream>

#ifndef OBJECT_CONSTRUCTION_HPP
#define OBJECT_CONSTRUCTION_HPP
class dynamicArray{
public:
  dynamicArray();
  dynamicArray(int);
  dynamicArray(const dynamicArray&);
  ~dynamicArray();
  dynamicArray& operator=(const dynamicArray&);
private:
  int *ptr;
};
#endif
